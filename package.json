    "svelte-check": "^4.1.4",
    "svelte-preprocess": "^6.0.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "@inertiajs/svelte": "^2.0.8"
  }
